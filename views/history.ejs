
<div id="historyContainer" class="container">
    <div id="title">
        <div id="historyTitle">ROOMS HISTORIC DATA</div><div class="back"><span class="fa fa-reply dark"></span><span class="light blue"> BACK TO </span><span id="back" class="bold dark">ROOMS</span></div>
        
    </div>

    <div id="history" class="row">
        <div id="leftCol" class="col-md-8">
            <div id="dateSelect">
                    <span> Date from </span>
                    <input class="date" type="text" name="initCalendar" id="initCalendar" placeholder="dd/mm/yyyy">
                    <span class="dateIcon button fa fa-calendar" id="initButton"></span>


                    <span> to </span> 
                    <input class="date" type="text" name="finalCalendar" id="finalCalendar" placeholder="dd/mm/yyyy">
                    <span class="dateIcon button fa fa-calendar" id="finalButton"></span>
                    
                    <span class="button" id="exportButton">Export List</span>

                    <span class="dateIcon button fa fa-search" id="searchButton"></span>

            </div>

            <div id="roomsInfo">
                <div class="row">
                    <div class="col-md-3">
                        <span>Sessions </span><span class="info bold dark"> 342341</span>
                    </div>
                    <div class="col-md-2">
                        <span>Rooms </span><span class="info bold dark">43</span>
                    </div>
                    <div class="col-md-2">
                        <span>Users </span><span class="info bold dark">564</span>
                    </div>
                    <div class="col-md-5">
                        <span>Time published </span><span class="info bold dark">714h 33m 30s</span>
                    </div>


                </div>



            </div>
        </div>
        <div id="rightCol" class="col-md-4">
            <div id="filtersContainer">
                <div class="row" id="search">
                   <div id="custom-search-input">
                        <div class="input-group first-row col-md-10">
                            <input id="searchBar" type="text" class="filter search-query form-control" placeholder="Search" />
                            <span class="input-group-btn">
                                <button class="btn btn-danger" type="button">
                                    <span class=" glyphicon glyphicon-search"></span>
                                </button>
                            </span>
                        </div>
                        <div class="first-row col-md-1">
                            <span class="dateIcon button fa fa-cog" id="configButton"></span>
                        </div>
                        <div class="first-row col-md-1">
                            <span class="collapsed fa fa-angle-down" id="collapseButtonDown"></span>
                            <span class="collapsed fa fa-angle-up" id="collapseButtonUp"></span>

                        </div>
                    </div>
                </div>
                <div id="filterCollapse">
                    <!-- Añadir esto -->
                    <div class="row filter_row" id="filter_row_0">
                        <div class="filters col-md-5">
                            <span>Key</span>
                            <input id="key" class="filter" type="text" placeholder="e.g. public">
                        </div>
                        <div class="filters col-md-5">
                            <span>Value</span>
                            <input id="value" class="filter" type="text" placeholder="e.g. true">
                        </div>
                        <div class="filters col-md-2">
                            <span class="button dateIcon fa fa-times removeButton" todelete="filter_row_0" id="remove_button_1"></span>
                        </div>
                    </div>


                    <!-- Hasta aquí -->

                    <div class="row filter_row" id="addFilter">
                        <div class="filters col-md-5">
                        </div>
                        <div class="filters col-md-5">
                        </div>
                        <div class="filters col-md-2">
                            <span class="button dateIcon fa fa-plus" id="addButton"></span>
                        </div>
                    </div>

                    <div class="row filter_row" id="addFilter">
                        <div class="filters col-md-6">
                            <span class="button" id="deleteFiltersButton">Delete filters</span>
                        </div>
                        <div class="filters col-md-6">
                            <span class="button" id="applyFiltersButton">Apply filters</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>
